{
	"name": "togos/togvm",
	"repositories": [
		{
			"type": "package",
			"package": {
				"name": "togos/togvm-spec",
				"version": "0.3.2",
				"source": {
					"url": "https://github.com/TOGoS/TOGVM-Spec.git",
					"type": "git",
					"reference": "origin/master"
				}
			}
		}
	],
	"require": {
		"php": "~5.4",
		"earthit/php-common": "~0.0.11",
		"ryprop/nife": "~0.1"
	},
	"require-dev": {
		"phpunit/phpunit": "~3.7",
		"togos/togvm-spec": "0.3.*",
		"togos/php-n2r": "^0.0.5"
	},
	"autoload": {
		"psr-0": {
			"TOGoS_TOGVM_": "lib/",
			"TOGoS_TOGES_": "lib/"
		}
	},
	"autoload-dev": {
		"psr-0": {
			"TOGoS_TOGVM_": "test/",
			"TOGoS_TOGES_": "test/"
		}
	}
}
